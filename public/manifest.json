{
    "manifest_version": 3,
    "name": "Tab Time Tracker",
    "version": "1.0.0",
    "description": "Track time spent on each tab and improve your focus.",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs", "storage"],
    "host_permissions": ["<all_urls>"]
}
